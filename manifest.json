{
  "name": "Snooker Scores",
  "description": "A chrome extension to view snooker scores.",
  "manifest_version": 3,
  "version": "0.1",
  "action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage"
  ]
}